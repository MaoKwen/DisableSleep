<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <RuntimeIdentifier>win-x64</RuntimeIdentifier>
    <UseWindowsForms>true</UseWindowsForms>
    <PublishSingleFile>true</PublishSingleFile>
    <SelfContained>false</SelfContained>
    <PublishTrimmed>false</PublishTrimmed>
    <PublishReadyToRun>false</PublishReadyToRun>
    <PublishReadyToRunShowWarnings>false</PublishReadyToRunShowWarnings>
    <EnableDefaultEmbeddedResourceItems>false</EnableDefaultEmbeddedResourceItems>
  </PropertyGroup>
  <ItemGroup>
    <EmbeddedResource Include="icons/allow-sleep-dark.ico" LogicalName="Icon.allow-sleep-dark" />
    <EmbeddedResource Include="icons/allow-sleep-light.ico" LogicalName="Icon.allow-sleep-light" />
    <EmbeddedResource Include="icons/disable-sleep-dark.ico" LogicalName="Icon.disable-sleep-dark" />
    <EmbeddedResource Include="icons/disable-sleep-light.ico" LogicalName="Icon.disable-sleep-light" />
  </ItemGroup>
</Project>
